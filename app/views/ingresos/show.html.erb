<%- model_class = Ingreso -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:nombre) %>:</strong></dt>
  <dd><%= @ingreso.nombre %></dd>
  <dt><strong><%= model_class.human_attribute_name(:documento) %>:</strong></dt>
  <dd><%= @ingreso.documento %></dd>
  <dt><strong><%= model_class.human_attribute_name(:ficha) %>:</strong></dt>
  <dd><%= @ingreso.ficha %></dd>
  <dt><strong><%= model_class.human_attribute_name(:elemento_id) %>:</strong></dt>
  <dd><%= @ingreso.elemento_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:prestamo) %>:</strong></dt>
  <dd><%= @ingreso.prestamo %></dd>
</dl>

<%= link_to t('.back', :default => t("helpers.links.back")),
              ingresos_path, :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_ingreso_path(@ingreso), :class => 'btn btn-default' %>
<%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              ingreso_path(@ingreso),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
